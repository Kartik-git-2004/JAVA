public class Main{
    public static void main(String[] args){
        int [] arr={1,2,3,4,5,6,8,9};
        int n = arr.length+1;
        int arr_sum = n*(n+1)/2;
        int sum =0;
        for(int ar : arr){
            sum = sum+ar;
        }
        int res = arr_sum-sum;
        System.out.println(res);
    }
}